a:
    ah/aah/aaah(f);
    # oh/ohh/ohhh(f);
    # ha/haa/haaa(f);
    # eh/ehh/ehhh(f);
    # um/umm/ummm(f);
    # oy/oyy/oyyy(f);

    `[`placed after something for emphasis or emotion`]`(particle def);

akesi:
    reptile(n);
    amphibian(n);
    scaly(adj material) creature(n);
    crawling(adj opinion gerund-like) creature(n);

    reptilian(adj qualifier);
    amphibian(adj qualifier);

ala:
    not(d negative);
    not(adv negative);
    nothing(n plural);
    none(n plural);
    no(d quantifier plural);

    `[`negates a word or phrase`]`(particle def);
    `[`forms a yes-no question`]`(particle def);

    # 0(num);
        # While technically true, this just floods the output. ilo Token
        # prioritizes numerals at the moment which is bad because normal
        # negation are more interesting. Thanks soweli Eweke for pointing this
        # out!

